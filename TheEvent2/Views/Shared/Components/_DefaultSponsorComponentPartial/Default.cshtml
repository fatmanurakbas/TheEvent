@model List<TheEvent.DAL.Entities.Sponsor>

<section id="sponsors" class="sponsors section light-background">

    <!-- Section Title -->
    <div class="container section-title" data-aos="fade-up">
        <h2>Bizi Destekleyen Güçlü Markalar</h2>
        <p>Etkinliğimizin gerçekleşmesine katkı sağlayan değerli sponsorlarımıza teşekkür ederiz. Onların desteğiyle teknoloji ve ilham dolu bir deneyim sunuyoruz.</p>
    </div><!-- End Section Title -->

    <div class="container" data-aos="fade-up" data-aos-delay="100">

        <div class="row g-0 clients-wrap">
          @foreach (var item in Model)
          {
            <div class="col-xl-3 col-md-4 client-logo">
                <img src="@item.ImageUrl" class="img-fluid" alt="">
            </div><!-- End Client Item -->
          }
        </div>

    </div>

</section>